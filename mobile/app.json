{
  "name": "gradus-mobile",
  "slug": "gradus-mobile",
  "version": "1.0.0",
  "orientation": "portrait",
  "icon": "./assets/icon.png",
  "userInterfaceStyle": "light",
  "scheme": "gradus",
  "newArchEnabled": true,
  "splash": {
    "image": "./assets/splash-icon.png",
    "resizeMode": "contain",
    "backgroundColor": "#3B82F6"
  },
  "ios": {
    "supportsTablet": true,
    "bundleIdentifier": "com.gradusindia.mobile"
  },
  "android": {
    "adaptiveIcon": {
      "foregroundImage": "./assets/adaptive-icon.png",
      "backgroundColor": "#3B82F6"
    },
    "package": "com.gradusindia.mobile"
  },
  "web": {
    "favicon": "./assets/favicon.png"
  },
  "extra": {
    "eas": {
      "projectId": "your-project-id"
    }
  },
  "plugins": [
    "expo-secure-store"
  ]
}